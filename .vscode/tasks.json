{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Patch Loader: Debug",
      "type": "shell",
      "command": "taskkill /IM 'GeometryDash.exe' /F; cmake --build build --config RelWithDebInfo --target ALL_BUILD; explorer steam://rungameid/322170",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "always"
      }
    },
    {
      "label": "Patch Loader: Release",
      "type": "shell",
      "command": "cmake --build build --config Release --target ALL_BUILD",
      "group": "build",
      "presentation": {
        "reveal": "always"
      }
    }
  ]
}